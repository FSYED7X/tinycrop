{
  "name": "jscrop",
  "version": "1.3.1",
  "description": "JavaScript Image Cropping",
  "main": "./lib/Crop.js",
  "scripts": {
    "test": "standard",
    "lint": "standard",
    "fix": "standard --fix",
    "build": "BABEL_ENV=development browserify src/Crop.js -s Crop -o dist/Crop.js -d",
    "min": "BABEL_ENV=production NODE_ENV=production browserify src/Crop.js -s Crop | uglifyjs -c > dist/Crop.min.js",
    "watch": "BABEL_ENV=development watchify src/Crop.js -s Crop -o dist/Crop.js -d -v",
    "react-example-watch": "BABEL_ENV=development watchify src/react-example.jsx -o dist/react-example.js -d -v",
    "react-example-min": "BABEL_ENV=production NODE_ENV=production browserify src/react-example.jsx | uglifyjs -c > dist/react-example.min.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "standard": {
    "ignore": [
      "/dist/",
      "/lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WebSeed/crop.git"
  },
  "keywords": [
    "image",
    "crop",
    "react"
  ],
  "author": "Will Bamford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WebSeed/crop/issues"
  },
  "homepage": "https://github.com/WebSeed/crop",
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "reactify": "^1.1.1",
    "standard": "^8.5.0",
    "uglifyify": "^3.0.4",
    "watchify": "^3.6.1"
  }
}
