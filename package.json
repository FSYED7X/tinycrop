{
  "name": "tinycrop",
  "version": "1.3.2",
  "description": "Lightweight pure JavaScript image crop",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "test": "standard",
    "lint": "standard",
    "fix": "standard --fix",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build:umd": "BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/tinycrop.js",
    "build:umd:min": "BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/tinycrop.min.js",
    "___TODO___ example:min": "BABEL_ENV=production NODE_ENV=production browserify examples/react/index.jsx | uglifyjs -c > examples/react/bundle.min.js"
  },
  "standard": {
    "ignore": [
      "/dist/",
      "/lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WebSeed/tinycrop.git"
  },
  "keywords": [
    "crop",
    "resize",
    "selection",
    "image",
    "thumbnail",
    "react",
    "lightweight",
    "pure"
  ],
  "author": "Will Bamford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WebSeed/tinycrop/issues"
  },
  "homepage": "https://github.com/WebSeed/tinycrop",
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "standard": "^8.5.0"
  }
}
