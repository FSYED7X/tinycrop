{
  "name": "tinycrop",
  "version": "1.3.1",
  "description": "Lightweight pure JavaScript image crop",
  "main": "lib",
  "scripts": {
    "test": "standard",
    "lint": "standard",
    "fix": "standard --fix",
    "lib": "BABEL_ENV=production babel src -d lib",
    "build": "BABEL_ENV=development browserify src/index.js -s tinycrop -o dist/tinycrop.js -d",
    "min": "BABEL_ENV=production NODE_ENV=production browserify src/index.js -s tinycrop | uglifyjs -c > dist/tinycrop.min.js",
    "watch": "BABEL_ENV=development watchify src/index.js -s tinycrop -o dist/tinycrop.js -d -v",
    "react-example-watch": "BABEL_ENV=development watchify examples/react/index.jsx -o examples/react/bundle.js -d -v",
    "react-example-min": "BABEL_ENV=production NODE_ENV=production browserify examples/react/index.jsx | uglifyjs -c > examples/react/bundle.min.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "standard": {
    "ignore": [
      "/dist/",
      "/lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WebSeed/tinycrop.git"
  },
  "keywords": [
    "crop",
    "resize",
    "selection",
    "image",
    "thumbnail",
    "react",
    "lightweight",
    "pure"
  ],
  "author": "Will Bamford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WebSeed/tinycrop/issues"
  },
  "homepage": "https://github.com/WebSeed/tinycrop",
  "dependencies": {
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "standard": "^8.5.0",
    "uglifyify": "^3.0.4",
    "watchify": "^3.6.1"
  }
}
